{
  "name": "oengine/laravel-package",
  "license": "MIT",
  "description": "support package base, merge plugin, hook filter action",
  "authors": [
    {
      "name": "Nguyen Van Hau",
      "email": "nguyenvanhau.soft@gmail.com",
      "role": "Developer",
      "homepage": "https://github.com/devhau"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "composer-plugin-api": "^1.1||^2.0"
  },
  "require-dev": {},
  "extra": {
    "laravel": {
      "providers": [],
      "aliases": {}
    },
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "class": "OEngine\\LaravelPackage\\Plugin\\MergePlugin"
  },
  "autoload": {
    "files": [
      "helpers/constraint.php",
      "helpers/package.php"
    ],
    "psr-4": {
      "OEngine\\LaravelPackage\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
